while语句
#include<stdio.h>
int main()
{
	int n = 0;//n = 多少自己定义
	int i = 0;
	int mux = 1;
	scanf_s("%d", &n);
	for (i = 1; i < n; i++)
	{
		mux *= (i + 1);
	}
	printf("mux = %d ", mux);
	return 0;
}
do-while语句
#include<stdio.h>
int main()
{

	int i = 0;
	scanf_s("%d", &i);
	int mux = i;
	do
	{

		mux *= (i - 1);
		i--;
		if (i != 1)
			continue;
		printf("mux=%d ", mux);
	} while ((i - 1) != 0);

	return 0;
}
